{
    "rewrites": [
      {
        "source": "/apis/:path*",
        "destination": "https://enk-api.com/apis/:path*"
      }
    ],
    "headers": [
      {
        "source": "/apis/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET,POST,PUT,DELETE,OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "X-Requested-With, Access-Control-Allow-Headers, Content-Type, Accept"
          }
        ]
      }
    ]
  }